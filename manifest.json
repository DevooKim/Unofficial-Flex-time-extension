{
  "manifest_version": 3,
  "name": "unofficial-flex-extension",
  "description": "Calculate Working Time From Flex",
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_title": "unofficial-flex-extension",
    "default_icon": {
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+F"
      }
    },
    "toggle_tab": {
      "suggested_key": {
        "default": "Alt+Shift+E"
      },
      "description": "근무-휴가 탭 전환"
    },
    "toggle_time": {
      "suggested_key": {
        "default": "Alt+Shift+T"
      },
      "description": "최소-남은 근무시간 전환"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "storage",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "src/pages/popup/index.html",
    "default_title": "unofficial-flex-extension"
  },
  "host_permissions": [
    "https://flex.team/*",
    "https://flex.team/api/v2/core/users/me/user-settings",
    "https://flex.team/api/v2/time-tracking/users/*/periods/work-schedules",
    "https://flex.team/api/v2/time-tracking/users/*/periods/work-schedules?*"
  ],
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  },
  "background": {
    "service_worker": "src/pages/background/index.ts"
  },
  "version": "3.4.0",
  "key": ""
}
